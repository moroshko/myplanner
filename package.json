{
  "name": "my-planner",
  "version": "1.0.0",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --write '**/*.{js,json}'",
    "deploy": "npm run build && cd build && now && now alias $(pbpaste) myplanner && cd .."
  },
  "dependencies": {
    "autosuggest-trie": "^2.1.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.0.0-alpha.27",
    "firebase": "^5.9.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "lodash.debounce": "^4.0.8",
    "lodash.groupby": "^4.6.0",
    "lodash.isequal": "^4.5.0",
    "lodash.memoize": "^4.1.2",
    "lodash.sortby": "^4.7.0",
    "lodash.throttle": "^4.1.1",
    "nanoid": "^2.0.1",
    "prettier": "^1.16.4",
    "query-string": "^6.4.2",
    "react": "^16.8.5",
    "react-autosuggest": "^9.4.3",
    "react-beautiful-dnd": "^10.1.1",
    "react-dom": "^16.8.5",
    "react-scripts": "^2.1.8",
    "react-select": "^2.4.2",
    "react-spring": "^8.0.18",
    "react-switch": "^4.1.0",
    "react-window": "^1.7.1",
    "react-window-infinite-loader": "^1.0.3"
  },
  "devDependencies": {
    "eslint-plugin-react-hooks": "1.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": "eslint --max-warnings 0",
    "**/*.{js,json}": [
      "npm run prettier",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
